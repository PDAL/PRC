<?xml version="1.0" encoding="utf-8"?>
<Pipeline version="1.0">
  <Writer type="drivers.prc.writer">
    <Option name="pdf_filename">
	    ./temp/out.pdf
    </Option>
    <Option name="plugin">
	    ./libpdal_plugin_writer_prc.dylib
    </Option>    
    <Option name="prc_filename">
	    ./temp/out.prc
    </Option>
    <Option name="output_format">
      pdf
    </Option>
    <Option name="color_scheme">
      oranges
    </Option>
    <Option name="contrast_stretch">
      linear
    </Option>
      <Reader type="drivers.las.reader">
        <Option name="filename">
          ../Serpent Mound Model LAS Data.laz
        </Option>
      </Reader>
  </Writer>
</Pipeline>
